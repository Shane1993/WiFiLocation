<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.wifilocation">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_LOGS" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/App_Theme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/App_Theme"></activity>

        <activity android:name=".MeasureActivity"
            android:theme="@style/My_Theme"
            android:label="采集区域信息"/>
        <activity android:name=".LoginAty"></activity>

        <activity android:name=".EntranceAty">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SearchAty"
            android:theme="@style/My_Theme"
            android:label="@string/search_title" />

        <activity
            android:name=".SettingAty"
            android:label="@string/setting"
            android:theme="@style/My_Theme"></activity>

        <activity
            android:name=".LoginPasswordAty"
            android:theme="@style/My_Theme"
            android:label="设定登录密码"
            />
        <activity
            android:name=".ConnectPasswordAty"
            android:theme="@style/My_Theme"
            android:label="设定连接密码"
            />
        <activity android:name=".MyDevicesAty"
            android:theme="@style/My_Theme"
            android:label="我的设备"
            />

        <activity android:name=".CreateAreaAty"
            android:theme="@style/My_Theme"
            android:label="创建新区域"
            />
        <activity android:name=".MapAty"
            android:theme="@style/My_Theme"
            android:label="地图"
            />

    </application>

</manifest>
